<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="urn:mites-schema"
    elementFormDefault="qualified"
    targetNamespace="urn:mites-schema">
    
  <xsd:element name="ANNOTATION">
    <xsd:complexType>
      <xsd:sequence>
  
  		<xsd:element name="FILEINFO">
	    <xsd:complexType>
	      <xsd:sequence>
	        
	        
	        <xsd:element name="SUBJECT">
		    <xsd:complexType>
		      <xsd:attribute name="NAME" type="xsd:string" use="required" />
		    </xsd:complexType>
  		</xsd:element>
  
	          <xsd:element name="LOCATION">
		    <xsd:complexType>
		      <xsd:attribute name="NAME" type="xsd:string" use="required" />
		    </xsd:complexType>
  		</xsd:element>
  
	          <xsd:element name="OBSERVER">
		    <xsd:complexType>
		      <xsd:attribute name="NAME" type="xsd:string" use="required" />
		    </xsd:complexType>
  		</xsd:element>
  
	         <xsd:element name="NOTES">
	           <xsd:complexType>
	             <xsd:attribute name="NAME" type="xsd:string" use="required" />
	           </xsd:complexType>
  		</xsd:element>
  
	      </xsd:sequence>
	    </xsd:complexType>
  		</xsd:element>
  
    <xsd:element name="LABELS">
      <xsd:complexType>
        <xsd:sequence>
          
            <xsd:element name="CATEGORY" maxOccurs="unbounded">
	      <xsd:complexType>
	        <xsd:sequence>
	          <xsd:element name="LABEL" maxOccurs="unbounded">
		      <xsd:complexType>
		        <xsd:attribute name="NAME" type="xsd:string" use="required" />
		      </xsd:complexType>
  		</xsd:element>
	        </xsd:sequence>
	        <xsd:attribute name="PROPERTY" type="xsd:string" use="optional" />
	        <xsd:attribute name="NAME" type="xsd:string" use="required" />
	      </xsd:complexType>
  	</xsd:element>
  
        </xsd:sequence>
      </xsd:complexType>
  </xsd:element>  
  
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>