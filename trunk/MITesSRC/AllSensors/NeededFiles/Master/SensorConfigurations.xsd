<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="urn:mites-schema"
    elementFormDefault="qualified"
    targetNamespace="urn:mites-schema">
    
    <xsd:element name="CONFIGURATIONS">
    	<xsd:complexType>
    	<xsd:sequence>
    		<xsd:element name="SENSORSET" maxOccurs="unbounded">
	      		<xsd:complexType>
	        	<xsd:sequence>
	          		<xsd:element name="NAME" maxOccurs="1" minOccurs="1" />
	          		<xsd:element name="DESCRIPTION" maxOccurs="1" minOccurs="1" />
	          		<xsd:element name="FILE" maxOccurs="1" minOccurs="1" />  		  
	        	</xsd:sequence>
	      		</xsd:complexType>	     	      
  		</xsd:element>
   	</xsd:sequence>
	</xsd:complexType>
  </xsd:element>
</xsd:schema>
